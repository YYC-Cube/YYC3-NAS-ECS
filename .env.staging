# =====================================================
# YYC³ FRP管理系统 - 预发布环境配置
# 创建日期: 2026-01-03
# 版本: 1.0.0
# =====================================================

# 环境标识
NODE_ENV=staging
VITE_APP_ENV=staging

# API配置 - 预发布环境
VITE_API_BASE_URL=https://staging-api.0379.email
VITE_API_TIMEOUT=30000
VITE_API_RATE_LIMIT=100

# API代理配置
VITE_API_PROXY_ENABLED=false

# 邮件服务配置
VITE_MAIL_API_URL=https://staging-mail.0379.email
VITE_MAIL_REFRESH_INTERVAL=60

# LLM服务配置
VITE_LLM_API_URL=https://staging-llm.0379.email
VITE_LLM_DEFAULT_MODEL=gpt-4
VITE_LLM_MAX_TOKENS=1024

# Redis API配置
VITE_REDIS_API_URL=https://staging-redis.0379.email

# DDNS服务配置
VITE_DDNS_API_URL=https://staging-ddns.0379.email
VITE_DDNS_UPDATE_INTERVAL=300

# FRP服务配置
VITE_FRP_API_URL=https://staging-frp.0379.email
VITE_FRP_ADMIN_URL=https://staging-frp-admin.0379.email

# NAS服务配置
VITE_NAS_API_URL=https://staging-nas.0379.email

# WebSocket配置
VITE_WS_URL=wss://staging-api.0379.email
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RECONNECT_ATTEMPTS=5

# 认证配置
VITE_AUTH_JWT_SECRET=staging-jwt-secret-2026
VITE_AUTH_TOKEN_STORAGE=localStorage
VITE_AUTH_REFRESH_TOKEN_ENABLED=true

# 功能开关
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_TRACKING=true

# 日志配置
VITE_LOG_LEVEL=info
VITE_LOG_TO_CONSOLE=true
VITE_LOG_TO_SERVER=true

# 性能配置
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=300000
VITE_DEBOUNCE_DELAY=300

# UI配置
VITE_THEME=default
VITE_LANGUAGE=zh-CN
VITE_TIMEZONE=Asia/Shanghai

# 开发工具配置
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_HOT_RELOAD=false
VITE_SOURCE_MAP=true
